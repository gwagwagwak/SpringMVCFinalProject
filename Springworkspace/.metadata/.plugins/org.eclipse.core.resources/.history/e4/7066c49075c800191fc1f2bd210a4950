<!-- namespace는 메퍼 인터페이스의 패키지 + 파일명을 의미한다. -->
<!-- DAO에서 이 메퍼를 'member'이라 한다. -->
<!-- <mapper namespace="testtest"> -->
<mapper namespace="com.test.paytesting.service.TestService">
	<select id="test" resultType="com.test.paytesting.dto.TestDTO">
		select * from tests where name='you'
	</select>


</mapper>
